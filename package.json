    "@types/express": "^4.17.13",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-serve-static-core": "^4.17.24",
    "@types/graphql-type-json": "^0.3.2",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "@types/node": "^15.14.1",
