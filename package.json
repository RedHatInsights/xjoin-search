    "coverage": "npm run compile && npm run seed && NODE_ENV=test LOG_LEVEL=fatal jest -i --coverage",
    "docs": "graphql-markdown src/schema/schema.graphql > docs/schema.md",
    "generate": "graphql-codegen --config codegen.yml",
    "lint": "eslint 'src/**/*.ts'",
    "seed": "ts-node test/seed.js",
    "start": "tsc && LOG_PRETTY=true ts-node-dev --respawn src/app.ts",
    "test": "npm run compile && npm run seed && NODE_ENV=test LOG_LEVEL=fatal jest -i",
    "verify": "npm run docs && npm run lint && npm run coverage",
    "preinstall": "npx npm-force-resolutions",
    "updateFromSchema": "cd scripts && ts-node update_from_schema.ts"
  },
  "author": "Jozef Hartinger",
