    "@typescript-eslint/eslint-plugin": "^4.28.3",
    "@typescript-eslint/parser": "^4.28.3",
    "apollo-server-types": "^3.0.0",
    "eslint": "^7.30.0",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-security": "^1.4.0",
    "graphql-markdown": "^6.0.0",
    "jest": "^27.1.1",
    "jest-each": "^27.0.6",
    "npm-force-resolutions": "0.0.10",
    "npm-run-all": "^4.1.5",
    "should": "^13.2.3",
