{
  "name": "xjoin-search",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rm -r dist",
    "compile": "npm run generate && tsc",
    "coverage": "npm run compile && npm run seed && NODE_ENV=test LOG_LEVEL=fatal jest -i --coverage",
    "docs": "graphql-markdown src/schema/schema.graphql > docs/schema.md",
    "generate": "graphql-codegen --config codegen.yml",
    "lint": "eslint 'src/**/*.ts'",
    "seed": "ts-node test/seed.js",
    "start": "tsc && LOG_PRETTY=true ts-node-dev --respawn src/app.ts",
    "test": "npm run compile && npm run seed && NODE_ENV=test LOG_LEVEL=fatal jest -t 'os_kernel_version'",
    "verify": "npm-run-all docs lint coverage",
    "preinstall": "npx npm-force-resolutions",
    "updateFromSchema": "node scripts/update_from_schema.js platformSchemas/systemProfileSchema.yaml"
  },
  "author": "Jozef Hartinger",
  "devDependencies": {
    "@graphql-codegen/cli": "^1.21.7",
